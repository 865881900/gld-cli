{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "install:husky": "is-ci || husky install",
    "treer": "treer -e ./result.txt -i \"/node_|.git|.vscode|.husky|.idea|static/\"",
    "dll": "webpack --config ./webpack/webpack.dll.js",
    "build": "npm run dll && cross-env ENV_NODE=production webpack --config ./webpack/webpack.prod.js",
    "build:speed": "npm run dll && cross-env ENV_NODE=production-speed webpack --config ./webpack/webpack.prod.js",
    "build:dev": "npm run dll && cross-env ENV_NODE=production-dev webpack --config ./webpack/webpack.prod.dev.js",
    "dev": "npm run serve",
    "serve": "cross-env ENV_NODE=development  webpack serve --config ./webpack/webpack.config.js",
    "loca": "cd ./server && npm run server"
  },
